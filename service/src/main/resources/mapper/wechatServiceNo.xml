<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wechatServiceNo">

	<select id="loadAll" parameterType="map" resultType="com.blog.blogsit.model.wechat.WechatServiceNo">
		select
			open_id as openId,
			wechat_no as wechatNo,
			token,
			app_id as appId,
			app_secret as appSecret
		from 
			wechat_serviceno
	</select>

</mapper>